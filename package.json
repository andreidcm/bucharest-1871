{
  "name": "bucharest-1871",
  "version": "0.1.0",
  "description": "Bucharest in 1871",
  "homepage": "https://github.com/andreidcm/bucharest-1871",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreidcm/bucharest-1871.git"
  },
  "bugs": {
    "url": "https://github.com/andreidcm/bucharest-1871/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "main": "src/index.html",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTILS------------------------------------": "",
    "clean": "rm -rf dist/* && rm -rf ./node_modules && rm -rf ./flow-typed",
    "setup": "npm run clean && npm i && flow-typed install && npm audit fix",
    "update": "npm-check -u",
    "----DEV--------------------------------------": "",
    "start:mock": "json-server --watch mock-db.json",
    "start": "parcel src/index.html",
    "----BUILD-----------------------------------": "",
    "build": "parcel build src/index.html",
    "build:babel": "babel src --out-dir compile",
    "prepublishOnly": "npm audit fix && npm test && npm run lint",
    "----LINT-------------------------------------": "",
    "lint:css": "stylelint src/**/*.css",
    "lint:flow": "flow",
    "lint:md": "markdownlint *.md",
    "lint:js": "eslint --ext .jsx,.js src",
    "lint": "npm run lint:md && npm run lint:css && npm run lint:js",
    "----TEST-------------------------------------": "",
    "test": "tape -r @babel/register -r ignore-styles src/**/*.test.jsx",
    "tdd": "nodemon -e js,jsx,json,css --watch src --exec \"npm run test | tap-diff\""
  },
  "dependencies": {
    "@asd14/m": "0.15.2",
    "@asd14/redux-all-is-list": "0.2.0",
    "classnames": "2.2.6",
    "content-type": "1.0.4",
    "cuid": "2.1.4",
    "debug": "4.1.0",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "redux": "4.0.1",
    "sanitize.css": "8.0.0"
  },
  "devDependencies": {
    "@asd14/eslint-config": "3.0.2",
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.1.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-compat": "2.6.3",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-html": "5.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.2.1",
    "eslint-plugin-jsx-control-statements": "2.2.1",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-unicorn": "6.0.1",
    "flow-bin": "0.86.0",
    "ignore-styles": "5.0.1",
    "json-server": "0.14.0",
    "jsx-control-statements": "3.2.8",
    "markdownlint-cli": "0.13.0",
    "nodemon": "1.18.6",
    "npm-check": "5.9.0",
    "parcel-bundler": "1.10.3",
    "postcss": "7.0.6",
    "postcss-custom-properties": "8.0.9",
    "postcss-modules": "1.4.1",
    "prettier": "1.15.2",
    "react-test-renderer": "16.6.3",
    "stylelint": "9.8.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "2.0.0",
    "tap-diff": "0.1.1",
    "tape": "4.9.1"
  }
}
